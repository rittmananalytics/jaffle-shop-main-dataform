config {
  type: "table"
}

with

source as (

    select * from ${ref('source_raw_stores')}

),

renamed as (

    select

        ----------  ids
        id as location_id,

        ---------- text
        name as location_name,

        ---------- numerics
        tax_rate,

        ---------- timestamps
        DATE_TRUNC('day', opened_at) as opened_date

    from source

)

select * from renamed